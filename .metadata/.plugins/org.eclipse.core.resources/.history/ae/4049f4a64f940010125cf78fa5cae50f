    <select id="selectBoardAllCount" resultType="_int">
      SELECT COUNT(ID)
        FROM BOARD
    </select>
    <select id="selectBoardList"
            resultType="com.ktdsuniversity.edu.board.vo.BoardVO">
      SELECT ID
		   , SUBJECT
		   , CONTENT
		   , EMAIL
		   , VIEW_CNT
		   , CRT_DT
		   , MDFY_DT
		   , DEL_YN
		  FROM BOARD
    </select>
